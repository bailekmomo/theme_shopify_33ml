            "custom_liquid_qdkWzq": {
              "type": "custom-liquid",
              "name": "t:names.custom_liquid",
              "disabled": true,
              "settings": {
                "custom_liquid": "{% if product.metafields.custom.notes_olfactives %}\n    <div class=\"luxury-notes-container\">\n      <div class=\"notes-header\">\n        <h3 class=\"notes-title\">Profil Olfactif</h3>\n        <div class=\"notes-divider\"></div>\n      </div>\n      \n      <div class=\"notes-luxe-grid\">\n        {% assign notes = product.metafields.custom.notes_olfactives.value  %}\n        {% for note in notes %}\n          {% assign note_trimmed = note | strip %}\n          <div class=\"luxe-note-item\">\n            <div class=\"luxe-note-icon\">\n              {% case note_trimmed %}\n                \n                {% when 'Rose' %}\n                  {% render 'icon-note-rose' %}\n               \n  \n                {% when 'Musc' %}\n                  {% render 'icon-note-musc' %}\n                  \n  \n                {% when 'Ambre' %}\n                  {% render 'icon-note-ambre' %}\n  \n                {% when 'Vanille' %}\n                  {% render 'icon-note-vanille' %}\n  \n                {% when 'Bois' %}\n                  {% render 'icon-note-bois' %}\n                {% when 'Agrumes' %}\n                  {% render 'icon-note-agrumes' %}\n                  \n  \n                {% when 'Floral' %}\n                  {% render 'icon-note-floral' %}\n                  \n  \n                {% when 'Épicé' %}\n                  {% render 'icon-note-epice' %}\n  \n                {% when 'Océan' %}\n                  {% render 'icon-note-ocean' %}\n                  \n  \n              {% endcase %}\n            </div>\n            <span class=\"luxe-note-label\">{{ note_trimmed }}</span>\n          </div>\n        {% endfor %}\n      </div>\n    </div>\n  \n    <style>\n      .luxury-notes-container {\n        margin: 40px 0;\n        padding: 30px 0;\n        border-top: 1px solid #e5e5e5;\n        border-bottom: 1px solid #e5e5e5;\n      }\n  \n      .notes-header {\n        display: flex;\n        align-items: center;\n        gap: 15px;\n        margin-bottom: 30px;\n      }\n  \n      .notes-title {\n        font-size: 11px;\n        font-weight: 600;\n        letter-spacing: 2.5px;\n        text-transform: uppercase;\n        color: #06193F;\n        margin: 0;\n        white-space: nowrap;\n      }\n  \n      .notes-divider {\n        flex: 1;\n        height: 1px;\n        background: linear-gradient(to right, #06193F, transparent);\n      }\n  \n      .notes-luxe-grid {\n        display: grid;\n        grid-template-columns: repeat(auto-fit, minmax(85px, 1fr));\n        gap: 30px;\n        padding: 0 10px;\n      }\n  \n      .luxe-note-item {\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        text-align: center;\n        opacity: 0;\n        animation: luxeFadeIn 0.8s cubic-bezier(0.4, 0, 0.2, 1) forwards;\n        cursor: pointer;\n        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\n      }\n  \n      .luxe-note-item:nth-child(1) { animation-delay: 0.1s; }\n      .luxe-note-item:nth-child(2) { animation-delay: 0.15s; }\n      .luxe-note-item:nth-child(3) { animation-delay: 0.2s; }\n      .luxe-note-item:nth-child(4) { animation-delay: 0.25s; }\n      .luxe-note-item:nth-child(5) { animation-delay: 0.3s; }\n      .luxe-note-item:nth-child(6) { animation-delay: 0.35s; }\n      .luxe-note-item:nth-child(7) { animation-delay: 0.4s; }\n      .luxe-note-item:nth-child(8) { animation-delay: 0.45s; }\n  \n      @keyframes luxeFadeIn {\n        from {\n          opacity: 0;\n          transform: translateY(20px) scale(0.95);\n        }\n        to {\n          opacity: 1;\n          transform: translateY(0) scale(1);\n        }\n      }\n  \n      .luxe-note-icon {\n        width: 70px;\n        height: 70px;\n        margin-bottom: 14px;\n        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        position: relative;\n      }\n  \n      .luxe-note-icon::before {\n        content: '';\n        position: absolute;\n        inset: -8px;\n        border-radius: 50%;\n        background: radial-gradient(circle, rgba(6, 25, 63, 0.05) 0%, transparent 70%);\n        opacity: 0;\n        transition: opacity 0.4s ease;\n      }\n  \n      .luxe-note-item:hover .luxe-note-icon::before {\n        opacity: 1;\n      }\n  \n      .luxe-note-icon svg {\n        width: 100%;\n        height: 100%;\n      }\n  \n      .icon-elegant {\n        fill: #06193F;\n        stroke: #06193F;\n        transition: all 0.4s ease;\n      }\n  \n      .luxe-note-item:hover .luxe-note-icon {\n        transform: scale(1.15) translateY(-3px);\n      }\n  \n      .luxe-note-item:hover .icon-elegant {\n        fill: #0a2559;\n        stroke: #0a2559;\n      }\n  \n      .luxe-note-label {\n        font-size: 12px;\n        font-weight: 500;\n        color: #06193F;\n        letter-spacing: 0.8px;\n        line-height: 1.4;\n        transition: all 0.3s ease;\n        opacity: 0.85;\n      }\n  \n      .luxe-note-item:hover .luxe-note-label {\n        font-weight: 600;\n        opacity: 1;\n        letter-spacing: 1px;\n      }\n  \n      @media (max-width: 1024px) {\n        .notes-luxe-grid {\n          grid-template-columns: repeat(auto-fit, minmax(75px, 1fr));\n          gap: 25px;\n        }\n  \n        .luxe-note-icon {\n          width: 65px;\n          height: 65px;\n        }\n      }\n  \n      @media (max-width: 768px) {\n        .luxury-notes-container {\n          margin: 30px 0;\n          padding: 20px 0;\n        }\n  \n        .notes-header {\n          margin-bottom: 20px;\n        }\n  \n        .notes-title {\n          font-size: 10px;\n          letter-spacing: 2px;\n        }\n  \n        .notes-luxe-grid {\n          grid-template-columns: repeat(auto-fit, minmax(65px, 1fr));\n          gap: 20px;\n          padding: 0;\n        }\n  \n        .luxe-note-icon {\n          width: 60px;\n          height: 60px;\n          margin-bottom: 10px;\n        }\n  \n        .luxe-note-label {\n          font-size: 11px;\n        }\n      }\n  \n      @media (max-width: 480px) {\n        .notes-luxe-grid {\n          grid-template-columns: repeat(auto-fit, minmax(55px, 1fr));\n          gap: 15px;\n        }\n  \n        .luxe-note-icon {\n          width: 55px;\n          height: 55px;\n        }\n  \n        .luxe-note-label {\n          font-size: 10px;\n        }\n      }\n    </style>\n  {% endif %}"
